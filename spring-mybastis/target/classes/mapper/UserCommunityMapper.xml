<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cdc.springmybastis.mapper.UserCommunityMapper">
    <insert id="insert" keyProperty="id" useGeneratedKeys="true" parameterType="com.cdc.springmybastis.model.UserCommunity">
        insert into user_community (account_id,name ,token,gmt_create,gmt_modified)
        values(#{account_id},#{name},#{token},#{gmt_create},#{gmt_modified})
    </insert>
    <select id="findByToken" resultType="com.cdc.springmybastis.model.UserCommunity" parameterType="com.cdc.springmybastis.model.UserCommunity">
        select token from user_community where token=#{token}

    </select>
</mapper>
